{% block html_head_container %}

{% include 'html-header.twig' %}
{% block head %}
{% endblock %}
</head>
{% endblock %}

    <body class="{{body_class}}" data-template="base.twig">
        {% include ('header.twig') %}

        <section id="content" role="main" class="container content-wrapper">
            {% if title %}<h1>{{title}}</h1>{% endif %}
            <div class="row">
                <div class="col-xs-12 wrapper {{sidebar_class}}">
                    {% block content %}
                    {{ 'Sorry, no content'|translate }}
                    {% endblock %}
                </div>
            </div>
            {% if sidebar %}
                <aside class="layout-sidebar">
                    {{sidebar}}
                </aside>
            {% endif %}
            {% block footer %}
            <footer id="footer">
                {% include 'footer.twig' %}
            </footer>
            {{ function('wp_footer') }}
            {% endblock %}
        </section>
    </body>
</html>
